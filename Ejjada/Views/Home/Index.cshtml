@model Ejjada.Controllers.HomeController.maz

@{
    ViewBag.Title = "Home Page";
}
@section scripts{
    <script>
        //Bxslider Slider
        $('.bxslider').bxSlider({
            pager: false,
            minSlides: 2,
            maxSlides: 4,
            slideWidth: 270,
            slideMargin: 30
        });
    </script>
    <script>
        $('.bxslider').children().on("mouseenter", function (e) {
            $('.bxslider').children().removeClass("bxslider-active");
            $('.bxslider-description').children().removeClass("description-active");
            var number = parseInt($(this).addClass("bxslider-active").data('order'));
            $('.bxslider-description').children().eq(--number).addClass("description-active");
        });
    </script>
}
<section class="container-full">
    <div class="container-full">
        <!-- slider start -->
        <div id="slidingcarousel" class="carousel slide">
            <div class="carousel-inner">
                <div class="item active">
                    <img src="~/Content/Tem/slider-img/plane-bg.jpg" alt="Aircraft" class="img-responsive" />
                    <div class="carousel-caption">
                        <h1 class="pull-right">Aircraft Cleaning!</h1>
                    </div>
                </div>
                <div class="item">
                    <img src="~/Content/Tem/slider-img/building-bg.jpg" alt="building" class="img-responsive" />
                    <div class="carousel-caption">
                        <h1>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Rope Access!</h1>
                    </div>
                </div>
                <div class="item">
                    <img src="~/Content/Tem/slider-img/road-bg.jpg" alt="Road" class="img-responsive" />
                    <div class="carousel-caption">
                        <h1 class="pull-right">Road Sign Cleaning!</h1>
                    </div>
                </div>
            </div>
            <a class="carousel-control left" href="#slidingcarousel" data-slide="prev"><span class="glyphicon glyphicon-chevron-left"></span></a>
            <a class="carousel-control right" href="#slidingcarousel" data-slide="next"><span class="glyphicon glyphicon-chevron-right"></span></a>
        </div>
        <!--slider end -->
    </div>
</section>
<div class="white-wrap">
    <!--**************************Begin Benefits*****************************-->
    <section class="container benefits-wrap grey-lines">
        <div class="row">
            <div class="col-xs-7 benefits">
                <h2 class="section-title">Service<span class="blue-text">Benefits</span></h2>
                @foreach (var item in Model.CBenefit)
                {
                    <ul class="camera_pag_ul">
