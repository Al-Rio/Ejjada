                    </div>
                </li>
                <li class="text-center" data-order="6">
                    <div class="bxslider-li-wrap">
                        <span class="multiple-users-icon"></span>
                        <h3>Construction Cleaning</h3>
                    </div>
                </li>
            </ul>
            <ul class="bxslider-description">
                @foreach (var item in Model.CService)
                {
                    <li class="description-active">
                        <p class="simple-text-14"><strong>@item.Service1</strong></p>
                    </li>
                    <li>
                        <p class="simple-text-14"><strong>@item.Service2</strong></p>
                    </li>
                    <li>
                        <p class="simple-text-14"><strong>@item.Service3</strong></p>
                    </li>
                    <li>
                        <p class="simple-text-14"><strong>@item.Service4</strong></p>
                    </li>
                    <li>
                        <p class="simple-text-14"><strong>@item.Service5</strong></p>
                    </li>
                    <li>
                        <p class="simple-text-14"><strong>@item.Service6</strong></p>
